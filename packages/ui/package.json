{
  "name": "@imbios/ui",
  "version": "0.1.1",
  "description": "Beautifully designed authentication UI components for better-auth",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "analyze": "tsup && pnpm dlx esbuild-visualizer --metadata ./dist/metafile.json --open",
    "size": "tsup && pnpm dlx size-limit"
  },
  "keywords": [
    "better-auth",
    "authentication",
    "auth",
    "ui",
    "components",
    "react",
    "shadcn",
    "radix-ui",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ImBIOS/ui",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/ImBIOS/ui/issues"
  },
  "homepage": "https://ui.imbios.dev",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "better-auth": "^1.4.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.6.2",
    "lucide-react": "^0.525.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^16.1.3",
    "tailwind-merge": "catalog:",
    "web-vitals": "^4.2.4",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@imbios/ui-config": "workspace:*",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.1.1",
    "jsdom": "^26.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.2",
    "vite": "^7.0.2",
    "vitest": "^3.1.1",
    "vitest-axe": "1.0.0-pre.5"
  }
}
